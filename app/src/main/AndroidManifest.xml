        <receiver
            android:name="com.rubyfood.features.location.GeofenceBroadcastReceiver"
            android:enabled="true"
            android:exported="true" />

        <receiver
            android:name="com.rubyfood.features.location.RestartBroadcast"
            android:enabled="true"
            android:exported="true" />

        <service android:name="com.rubyfood.features.location.GeofenceTransitionsIntentService" />

        <service
            android:name="com.rubyfood.features.location.GeofenceTransitionsJobIntentService"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <service
            android:name="com.rubyfood.features.dashboard.presentation.AlertJobIntentService"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <service
            android:name="com.rubyfood.features.location.LocationJobService"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <service
            android:name="com.rubyfood.features.geofence.GeofenceService"
            android:enabled="true" />

        <receiver
            android:name="com.rubyfood.features.location.GpsLocationReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="android.location.PROVIDERS_CHANGED" />
                <!--<action android:name="gps_toggle" />-->
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

        <service android:name="com.rubyfood.features.dashboard.presentation.AlertService" />

        <!--<service
            android:name=".features.dashboard.presentation.CollectionOrderAlertJobService"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />-->

        <!-- Firebase Notifications -->
        <service android:name="com.rubyfood.fcm.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name="com.rubyfood.fcm.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <!-- ./Firebase Notifications -->

        <service android:name=".features.login.presentation.MemberShopListIntentService"/>

        <service android:name=".features.micro_learning.presentation.FileOpeningTimeIntentService"/>




        <service android:name="com.rubyfood.MonitorService"
            android:enabled="true"
            android:exported="false">

        </service>

        <receiver
            android:name=".MonitorBroadcast"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>



    </application>

</manifest>